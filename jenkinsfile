pipeline {
    agent any

    stages {

        stage("build"){
            steps{
                echo "Building the project..."
                mvn clean
            }
        }

        stage("test"){
            steps{
                echo "Testing the project..."
                mvn test
            }
        }

        stage("deploy"){
            steps{
                echo "Deploying the project..."
            }
        }

    }
}